# Environment Variables for CentralHub
# Copy this file to .env and update with your values

# Application
GIN_MODE=debug
APP_PORT=8080

# MongoDB
MONGO_HOST=mongodb
MONGO_PORT=27017
MONGO_USERNAME=admin
MONGO_PASSWORD=admin123
MONGO_DATABASE=centralhub

# Volcengine (External Service)
VOLC_ACCESS_KEY=your-access-key
VOLC_SECRET_KEY=your-secret-key
VOLC_REGION=cn-beijing

# Logging
LOG_LEVEL=info
LOG_OUTPUT=stdout
